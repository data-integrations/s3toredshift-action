{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "S3ToRedshift Configuration",
      "properties": [
        {
          "widget-type": "password",
          "label": "S3 Access Key",
          "name": "accessKey"
        },
        {
          "widget-type": "password",
          "label": "S3 Secret Access Key",
          "name": "secretAccessKey"
        },
        {
          "widget-type": "password",
          "label": "S3 IAM Role",
          "name": "iamRole"
        },
        {
          "widget-type": "select",
          "label": "S3 Region",
          "name": "s3Region",
          "widget-attributes": {
            "values": [
              "us-east-1",
              "us-west-1",
              "us-west-2",
              "ap-southeast-1",
              "ap-southeast-2",
              "ap-northeast-1",
              "eu-west-1",
              "sa-east-1"
            ]
          }
        },
        {
          "widget-type": "textbox",
          "label": "S3 Data Path",
          "name": "s3DataPath"
        },
        {
          "widget-type": "textbox",
          "label": "JDBC Redshift Cluster Database URL",
          "name": "clusterDbUrl"
        },
        {
          "widget-type": "textbox",
          "label": "Redshift Master User",
          "name": "masterUser"
        },
        {
          "widget-type": "password",
          "label": "Redshift Master Password",
          "name": "masterPassword"
        },
        {
          "widget-type": "textbox",
          "label": "Redshift Table Name",
          "name": "tableName"
        },
        {
          "widget-type": "csv",
          "label": "List of Columns",
          "name": "listOfColumns"
        }
      ]
    }
  ],
  "outputs": []
}
